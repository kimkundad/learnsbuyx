{
  "cli": {
    "version": ">= 15.0.6",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "credentialsSource": "remote",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "credentialsSource": "remote",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "enterpriseProvisioning": "universal"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "promthephome@yahoo.com"
      }
    }
  }
}
